<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Bugfender.Sdk</id>
    <version>5.0.1</version>
    <title>Bugfender SDK for .NET MAUI</title>
    <authors>Bugfender Team</authors>
    <owners>Bugfender Team</owners>
    <readme>docs\README.md</readme>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="file">LICENSE.txt</license>
    <projectUrl>https://bugfender.com/</projectUrl>
    <description>Collect your iOS and Android application logs and send them to Bugfender for remote access. Now with .NET 10 and .NET 9 support and Bugfender SDK 3.6.0/2.2.0.</description>
    <summary>Collect your application logs and send them to Bugfender for remote access</summary>
    <releaseNotes>v5.0.1: Bugfixing</releaseNotes>
    <copyright>Beenario GmbH</copyright>
    <language></language>
    <tags>logs log xamarin serilog nlog log4net maui net10 net9</tags>
    <dependencies>
      <!-- .NET 10 Support -->
      <group targetFramework="net10.0-android36.0" />
      <group targetFramework="net10.0-ios15.0" />
      <!-- .NET 9 Support -->
      <group targetFramework="net9.0-android35.0" />
      <group targetFramework="net9.0-ios15.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="../README.md" target="docs\" />
    <file src="../LICENSE.txt" target="" />

    <!-- .NET 10 Android -->
    <file src="Android/bin/Release/net10.0-android/" target="lib/net10.0-android36.0" />

    <!-- .NET 10 iOS -->
    <file src="iOS/bin/Release/net10.0-ios/" target="lib/net10.0-ios15.0" />

    <!-- .NET 9 Android -->
    <file src="Android/bin/Release/net9.0-android/" target="lib/net9.0-android35.0" />

    <!-- .NET 9 iOS -->
    <file src="iOS/bin/Release/net9.0-ios/" target="lib/net9.0-ios15.0" />
  </files>
</package>
